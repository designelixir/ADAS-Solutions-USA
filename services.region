<!doctype html>
<html class="bg">
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    {squarespace-headers}
  </head>
  {@|apply SiteBlocks/googleAnalytics.block}
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
  <main class="content-container fade-in-delay" role="main" data-content-field="main-content">
    <squarespace:navigation navigationId="siteNav" template="site-navigation" />
      {@|apply SiteBlocks/pageHeader.block}

        {squarespace.main-content}

        {.if item}
        
        {.or}
        <section class="basic-padding" id="whatToExpectCards" style="z-index: 2; margin-top: 50px;">
            <squarespace:block-field id="WhatToExpect-Description" columns="1" />
            {@|apply SiteBlocks/completeAdasSolutionCards.block}
        </section>

        <section class="basic-padding banner-bg-reverse" style="margin-top: -175px; padding-top: 200px; z-index: 1;">
            <squarespace:block-field id="GetStarted-Description" columns="1" />
            <squarespace:block-field id="RequestAQuote" columns="1" />
        </section>

        <section>
            <squarespace:block-field id="EasyToDoBusiness-Description" columns="1" />
            {@|apply SiteBlocks/easyToDoBusiness.block}
        </section>
        

        {@|apply siteBlocks/callToAction.block}
        {.end}
    </main>
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    {squarespace-footers}
  </body>
</html>

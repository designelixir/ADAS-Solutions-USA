<style>
.book-header-title-item { min-width: 250px; max-width: 25%; padding: 5px 10px; border: 1px solid white; min-height: 75px;  margin: 1px; background: var(--red);}
.book-header-title { transform: translateY(-50px)  }

.book-header-title-item:hover {background: var(--darkred); cursor: pointer;}
.book-header-title-item h3 {color: white!important; line-height: 22px; font-size: 18px; text-align: center;}

.book-header-title-item:hover h3 {font-weight: 600;}

@media screen and (max-width: 860px) {
  .book-service-title-wrapper {flex-direction: column;}
  .book-header-title-item {width: 100%;}
}

</style>
<div class="book-header-title flex-center-center flex-wrap book-service-title-wrapper" style="margin-bottom: 25px;">
{.repeated section items}
    
        
        <div class="book-header-title-item flex-center-center">
          <a href="{fullUrl}" class="no-link-styling"><h3>{title}</h3></a>
        </div>
        
    
{.end}
</div>

<section>
  <div class="basic-padding" id="whatToExpectCards" style="z-index: 2;">
      <squarespace:block-field id="WhatToExpect-Description" columns="1" />
      {@|apply SiteBlocks/completeAdasSolutionCards.block}
  </div>
  <div class="banner-bg-reverse flex-center-center" style="margin-top: -100px; padding-top: 100px">
        <squarespace:block-field id="Consumer-HowItWorks-Title" columns="1" />
        
    </div>
    <div class="flex-center-center flex-column">
        <div style="max-width: 800px">   
            <squarespace:block-field id="Consumer-HowItWorks-Content" columns="1" />
        </div>
    </div>
</section>
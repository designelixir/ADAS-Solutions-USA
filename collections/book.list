<style>
.book-service-title-wrapper {transform: translateY(-50px); z-index: 2;}
.book-service-title {background: var(--darkgray); width: 19.5%; padding: 35px 0; min-height: 75px;  margin: 0.25%; color: white!important; text-decoration: unset; line-height: 22px; text-align: center;}
.book-service-title h3 {color: white; line-height: 20px;}
.book-service-title:hover {background: var(--ssgray); cursor: pointer;}

@media screen and (max-width: 860px) {
  .book-service-title-wrapper {flex-wrap: wrap;}
  .book-service-title {width: unset; padding: 35px 15px;}
}

</style>
<div class="book-service-title-wrapper flex-center-center flex-wrap">
{.repeated section items}
    
        <a class="book-service-title no-text-spacing" href="{fullUrl}"><h3 class=" no-text-spacing">{title}</h3></a>
    
{.end}
</div>